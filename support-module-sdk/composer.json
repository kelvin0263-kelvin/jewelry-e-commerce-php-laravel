{
  "name": "app/support-module-sdk",
  "description": "PHP SDK for consuming the Support module API (tickets, chat, FAQ)",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/support": "^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "SupportModule\\Sdk\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SupportModule\\Sdk\\SupportSdkServiceProvider"
      ],
      "aliases": {
        "SupportSdk": "SupportModule\\Sdk\\Facades\\SupportSdk"
      }
    }
  }
}

